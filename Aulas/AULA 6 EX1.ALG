algoritmo "Aula 6 ex1"
var
    ano_nasc, ano_atual, idade: inteiro

inicio

     escreval("Digite o ano atual: ")
     leia(ano_atual)
     escreva("Digite o ano que você nasceu: ")
     leia(ano_nasc)
     
     idade <- (ano_atual - ano_nasc)
     
     escreval("Você tem: ", idade, " anos de idade !")
     
     
fimalgoritmo