algoritmo "SomadorNumerico"
var

   contador, N, soma : Inteiro

inicio
   contador <- 1
   soma <- 0
   Enquanto (contador <= 5) faca
      escreva("DIGITE O ", contador, "° VALOR: ")
      leia(N)
      soma <- (n + soma)
      contador <- contador + 1
   FimEnquanto
   escreva("A SOMA DE TODOS OS VALORES FOI: ", soma)

fimalgoritmo