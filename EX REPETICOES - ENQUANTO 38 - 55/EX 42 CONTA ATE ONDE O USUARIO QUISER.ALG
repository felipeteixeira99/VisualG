algoritmo "EX 42 CONTA ATE ONDE O USUARIO QUISER"
var

   cont, num: Inteiro

inicio

   escreva("ATÉ QUE NÚMERO QUER CONTAR? ")
   leia(num)

   //num <- num + 1

   cont <- 0

   enquanto cont < num faca
      cont <- cont + 1
      escreva(cont,"...")
   Fimenquanto
   escreva(" Acabou !")

fimalgoritmo