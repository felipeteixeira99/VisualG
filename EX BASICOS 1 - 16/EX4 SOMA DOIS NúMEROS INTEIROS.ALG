algoritmo "ex4 Soma dois números inteiros"
var

   n1, n2: inteiro
   soma: inteiro

inicio
      escreval("Digite um valor: ")
      leia(n1)
      escreva("Digite outro valor: ")
      leia(n2)
      
      soma <- n1 + n2
      escreval("A soma entre ",n1, " e ",n2, " é igual a ",soma)
      
fimalgoritmo