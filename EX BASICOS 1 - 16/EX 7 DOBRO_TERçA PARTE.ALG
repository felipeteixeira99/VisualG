algoritmo "Ex 7 Dobro_Terça Parte"
var

dob, ter, num: Real


inicio

      escreva("DIGITE UM NÚMERO: ")
      leia(num)
      
      dob <- num * 2
      ter <- num / 3
      
      escreval("O DOBRO DE ",num," É ",dob:1:2)
      escreval("A TERÇA PARTE DE "," É ",ter:2:5)
      

fimalgoritmo